#include <iostream>
using namespace std;
int main() {
    //declare an array
  int fibonacci_numbers[8];
  // first element of fabonacci sequence is 0 and next is 1
  fibonacci_numbers[0] = 0;
  fibonacci_numbers[1] = 1;
// for store fabonacci numbers in fibonacci series
  for (int i = 2; i<8; ++i) {
    fibonacci_numbers[i] = fibonacci_numbers[i-1] + fibonacci_numbers[i-2];
  }
// To print Stored fabonacci numbers on Screen
  cout << "The first 8 Fibonacci numbers are: ";
  for (int i = 0; i<8; ++i) {
    cout << fibonacci_numbers[i] << " ";
  }
  cout << endl; //for new line
  return 0;
}
